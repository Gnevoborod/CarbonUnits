<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CarbonUnits</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CarbonUnits.styles.css" asp-append-version="false" />
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.bootstrap.css" />
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.bootstrap4.css" />
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.jqueryui.css" />
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.material.css" />
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.foundation.css" />
    
    <link rel="stylesheet" href="~/Content/DataTables/css/dataTables.uikit.css" />
</head>
<body>
@Html.Partial("_NavigationBar")
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - CarbonUnits - @*<a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>*@
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript" language="javascript" src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
